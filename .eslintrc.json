{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint"
  ],
  "globals": {
    "ADS_CONFIG": true,
    "__ASSERTS_ENABLED__": true,
    "__HOT_RELOADING_ENABLED__": true,
    "__SENTRY_DSN__": true,
    "__APP_STAMP__": true,
    "window": true
  },
  "plugins": [
    "react",
    "fp",
    "jsx-a11y",
    "import",
    "promise",
    "lodash-fp",
    "lodash"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "jsx": true
    }
  },
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "lodash-fp/consistent-compose": [1, "flow"],
    "lodash-fp/consistent-name": [1],
    "lodash-fp/no-argumentless-calls": [1],
    "lodash-fp/no-chain": [1],
    "lodash-fp/no-extraneous-args": [0],
    "lodash-fp/no-extraneous-function-wrapping": [1],
    "lodash-fp/no-extraneous-iteratee-args": [0],
    "lodash-fp/no-for-each": [0],
    "lodash-fp/no-partial-of-curried": [1],
    "lodash-fp/no-single-composition": [1],
    "lodash-fp/no-submodule-destructuring": [0],
    "lodash-fp/no-unused-result": [0],
    "lodash-fp/prefer-compact": [1],
    "lodash-fp/prefer-composition-grouping": [1],
    "lodash-fp/prefer-constant": [0],
    "lodash-fp/prefer-flat-map": [0],
    "lodash-fp/prefer-get": [1],
    "lodash-fp/prefer-identity": [1],
    "lodash-fp/preferred-alias": [1],
    "lodash-fp/use-fp": [0],

    "lodash/callback-binding": [1],
    "lodash/collection-method-value": [1],
    "lodash/collection-return": [1],
    "lodash/no-double-unwrap": [1],
    "lodash/no-extra-args": [1],
    "lodash/no-unbound-this": [1],
    "lodash/unwrap": [0],
    "lodash/chain-style": [0],
    "lodash/chaining": [1, "never"],
    "lodash/consistent-compose": [0],
    "lodash/identity-shorthand": [1, "always"],
    "lodash/import-scope": [1, "method"],
    "lodash/matches-prop-shorthand": [0],
    "lodash/matches-shorthand": [0],
    "lodash/no-commit": [1],
    "lodash/path-style": [1, "string"],
    "lodash/prefer-compact": [1],
    "lodash/prefer-filter": [1],
    "lodash/prefer-flat-map": [0],
    "lodash/prefer-invoke-map": [0],
    "lodash/prefer-map": [1],
    "lodash/prefer-reject": [1],
    "lodash/prefer-thru": [0],
    "lodash/pprefer-wrapper-method": [0],
    "lodash/preferred-alias": [1],
    "lodash/prop-shorthand": [0],
    "lodash/prefer-constant": [0],
    "lodash/prefer-get": [0],
    "lodash/prefer-includes": [1],
    "lodash/prefer-is-nil": [1],
    "lodash/prefer-lodash-chain": [0],
    "lodash/prefer-lodash-method": [0],
    "lodash/prefer-lodash-typecheck": [1],
    "lodash/prefer-matches": [1],
    "lodash/prefer-noop": [0],
    "lodash/prefer-over-quantifier": [1],
    "lodash/prefer-some": [1],
    "lodash/prefer-startswith": [1],
    "lodash/prefer-times": [1],

    "fp/no-arguments": [1],
    "fp/no-class": [0],
    "fp/no-delete": [1],
    "fp/no-events": [0],
    "fp/no-get-set": [1],
    "fp/no-let": [0],
    "fp/no-loops": [1],
    "fp/no-mutating-assign": [0],
    "fp/no-mutating-methods": [0],
    "fp/no-mutation": [0],
    "fp/no-nil": [0],
    "fp/no-proxy": [0],
    "fp/no-rest-parameters": [0],
    "fp/no-this": [0],
    "fp/no-throw": [0],
    "fp/no-unused-expression": [0],
    "fp/no-valueof-field": [0],

    "promise/always-return": [0],
    "promise/no-return-wrap": [0],
    "promise/param-names": [1],
    "promise/catch-or-return": [0],
    "promise/no-native": [0],
    "promise/no-nesting": [0],
    "promise/no-promise-in-callback": [0],
    "promise/no-callback-in-promise": [0],
    "promise/avoid-new": [0],
    "promise/prefer-await-to-then": [0],

    "import/no-unresolved": [1],
    "import/named": [1],
    "import/default": [1],
    "import/namespace": [0],
    "import/no-restricted-paths": [1],
    "import/no-absolute-path": [1],
    "import/no-dynamic-require": [1],
    "import/no-internal-modules": [0],
    "import/no-webpack-loader-syntax": [1],
    "import/export": [1],
    "import/no-named-as-default": [1],
    "import/no-named-as-default-member": [1],
    "import/no-deprecated": [1],
    "import/no-extraneous-dependencies": [1],
    "import/no-mutable-exports": [1],
    "import/unambiguous": [0],
    "import/no-commonjs": [0],
    "import/no-amd": [1],
    "import/no-nodejs-modules": [0],
    "import/first": [1],
    "import/no-duplicates": [1],
    "import/no-namespace": [1],
    "import/extensions": [0],
    "import/order": [1],
    "import/newline-after-import": [1],
    "import/prefer-default-export": [0],
    "import/max-dependencies": [0],
    "import/no-unassigned-import": [0],
    "import/no-named-default": [1],

    "jsx-a11y/anchor-has-content": [0],
    "jsx-a11y/aria-props": [0],
    "jsx-a11y/aria-proptypes": [0],
    "jsx-a11y/aria-role": [0],
    "jsx-a11y/aria-unsupported-elements": [0],
    "jsx-a11y/click-events-have-key-events": [0],
    "jsx-a11y/heading-has-content": [1],
    "jsx-a11y/html-has-lang": [0],
    "jsx-a11y/img-has-alt": [0],
    "jsx-a11y/img-redundant-alt": [0],
    "jsx-a11y/label-has-for": [1],
    "jsx-a11y/lang": [0],
    "jsx-a11y/mouse-events-have-key-events": [0],
    "jsx-a11y/no-access-key": [1],
    "jsx-a11y/no-marquee": [0],
    "jsx-a11y/no-onchange": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "jsx-a11y/onclick-has-focus": [0],
    "jsx-a11y/onclick-has-role": [0],
    "jsx-a11y/role-has-required-aria-props": [0],
    "jsx-a11y/role-supports-aria-props": [0],
    "jsx-a11y/scope": [0],
    "jsx-a11y/tabindex-no-positive": [1],
    "jsx-a11y/accessible-emoji": [0],
    "jsx-a11y/aria-activedescendant-has-tabindex": [0],
    "jsx-a11y/iframe-has-title": [0],
    "jsx-a11y/no-autofocus": [0],
    "jsx-a11y/no-distracting-elements": [0],
    "jsx-a11y/no-redundant-roles": [0],

    "react/display-name": [1],
    "react/forbid-component-props": [0],
    "react/forbid-prop-types": [0],
    "react/no-array-index-key": [1],
    "react/no-children-prop": [0],
    "react/no-danger": [0],
    "react/no-danger-with-children": [1],
    "react/no-deprecated": [1],
    "react/no-did-mount-set-state": [1],
    "react/no-did-update-set-state": [1],
    "react/no-direct-mutation-state": [1],
    "react/no-find-dom-node": [1],
    "react/no-is-mounted": [1],
    "react/no-multi-comp": [0],
    "react/no-render-return-value": [1],
    "react/no-set-state": [1],
    "react/no-string-refs": [0],
    "react/no-unescaped-entities": [1],
    "react/no-unknown-property": [1],
    "react/no-unused-prop-types": [1],
    "react/prefer-es6-class": [1],
    "react/prefer-stateless-function": [1],
    "react/prop-types": [1],
    "react/react-in-jsx-scope": [1],
    "react/require-default-props": [0],
    "react/require-optimization": [0],
    "react/require-render-return": [1],
    "react/self-closing-comp": [1],
    "react/sort-comp": [0],
    "react/sort-prop-types": [0],
    "react/style-prop-object": [1],
    "react/jsx-boolean-value": [1],
    "react/jsx-closing-bracket-location": [1],
    "react/jsx-curly-spacing": [1, "never", { "allowMultiline": true }],
    "react/jsx-equals-spacing": [1, "never"],
    "react/jsx-filename-extension": [0],
    "react/jsx-first-prop-new-line": [1, "multiline"],
    "react/jsx-handler-names": [1],
    "react/jsx-indent": [1, 2],
    "react/jsx-indent-props": [1, 2],
    "react/jsx-key": [1],
    "react/jsx-max-props-per-line": [0],
    "react/jsx-no-bind": [1],
    "react/jsx-no-comment-textnodes": [1],
    "react/jsx-no-duplicate-props": [1, { "ignoreCase": true }],
    "react/jsx-no-literals": [0],
    "react/jsx-no-target-blank": [1],
    "react/jsx-no-undef": [1],
    "react/jsx-pascal-case": [1, { "allowAllCaps": true }],
    "react/jsx-sort-props": [0],
    "react/jsx-tag-spacing": [1, { "closingSlash": "never", "beforeSelfClosing": "always", "afterOpening": "never" }],
    "react/jsx-uses-react": [1],
    "react/jsx-uses-vars": [1],
    "react/jsx-wrap-multilines": [1, { "declaration": true, "assignment": true, "return": true }],

    "no-constant-condition": [1],

    "class-methods-use-this": [0],

    "no-cond-assign": [1, "always"],
    "no-console": [1],
    "no-control-regex": [1],
    "no-debugger": [1],
    "no-dupe-args": [1],
    "no-dupe-keys": [1],
    "no-duplicate-case": [1],
    "no-empty": [1],
    "no-empty-character-class": [1],
    "no-ex-assign": [1],
    "no-extra-boolean-cast": [1],
    "no-extra-parens": [0],
    "no-extra-semi": [1],
    "no-func-assign": [1],
    "no-inner-declarations": [1],
    "no-invalid-regexp": [1],
    "no-irregular-whitespace": [1],
    "no-obj-calls": [1],
    "no-prototype-builtins": [0],
    "no-regex-spaces": [1],
    "no-sparse-arrays": [1],
    "no-template-curly-in-string": [1],
    "no-unexpected-multiline": [1],
    "no-unreachable": [1],
    "no-unsafe-finally": [1],
    "no-unsafe-negation": [1],
    "use-isnan": [1],
    "valid-jsdoc": [1, {
      "requireReturn": true,
      "requireParamDescription": false,
      "requireReturnDescription": false
    }],
    "valid-typeof": [1],

    "accessor-pairs": [1],
    "array-callback-return": [1],
    "block-scoped-var": [1],
    "complexity": [0, 5],
    "consistent-return": [1],
    "curly": [1],
    "default-case": [1],
    "dot-location": [1, "property"],
    "dot-notation": [1],
    "eqeqeq": [1, "always", {"null": "ignore"}],
    "guard-for-in": [1],
    "no-alert": [1],
    "no-caller": [1],
    "no-case-declarations": [1],
    "no-div-regex": [1],
    "no-else-return": [1],
    "no-empty-function": [0],
    "no-empty-pattern": [0],
    "no-eq-null": [0],
    "no-eval": [1],
    "no-extend-native": [1],
    "no-extra-bind": [1],
    "no-extra-label": [1],
    "no-fallthrough": [1],
    "no-floating-decimal": [1],
    "no-global-assign": [1],
    "no-implicit-coercion": [1],
    "no-implicit-globals": [1],
    "no-implied-eval": [1],
    "no-invalid-this": [1],
    "no-iterator": [1],
    "no-labels": [1],
    "no-lone-blocks": [1],
    "no-loop-func": [1],
    "no-magic-numbers": [0, { "ignoreArrayIndexes": true, "enforceConst": true, "detectObjects": false }],
    "no-multi-spaces": [1, { "exceptions": { "VariableDeclarator": true } }],
    "no-multi-str": [1],
    "no-native-reassign": [1],
    "no-new": [1],
    "no-new-func": [1],
    "no-new-wrappers": [1],
    "no-octal-escape": [1],
    "no-octal": [1],
    "no-param-reassign": [0, { "props": true }],
    "no-proto": [1],
    "no-redeclare": [1],
    "no-return-assign": [1],
    "no-script-url": [1],
    "no-self-assign": [1],
    "no-self-compare": [1],
    "no-sequences": [1],
    "no-throw-literal": [1],
    "no-unmodified-loop-condition": [1],
    "no-unused-expressions": [1],
    "no-unused-labels": [1],
    "no-useless-call": [1],
    "no-useless-concat": [1],
    "no-useless-escape": [1],
    "no-void": [1],
    "no-warning-comments": [0, { "terms": ["todo"], "location": "start" }],
    "no-with": [1],
    "radix": [1],
    "vars-on-top": [0],
    "wrap-iife": [1, "inside"],
    "yoda": [1, "never", { "exceptRange": true }],

    "strict": [0, "global"],

    "init-declarations": [0, "always"],
    "no-catch-shadow": [1],
    "no-delete-var": [1],
    "no-label-var": [1],
    "no-restricted-globals": [0],
    "no-shadow-restricted-names": [1],
    "no-shadow": [0],
    "no-undef": [1],
    "no-undef-init": [1],
    "no-undefined": [0],
    "no-unused-vars": [0, { "vars": "local", "args": "none", "varsIgnorePattern": "^React$" }],
    "no-use-before-define": [1],

    "callback-return": [0],
    "global-require": [1],
    "handle-callback-err": [1],
    "no-mixed-requires": [1],
    "no-new-require": [1],
    "no-path-concat": [1],
    "no-process-env": [0],
    "no-process-exit": [1],
    "no-restricted-modules": [0],
    "no-sync": [0],

    "array-bracket-spacing": [1, "always"],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs"],
    "camelcase": [0, {"properties": "never"}],
    "comma-dangle": [1, "never"],
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": [1],
    "computed-property-spacing": [1, "never"],
    "consistent-this": [0, "that"],
    "eol-last": [1],
    "func-call-spacing": [1, "never"],
    "func-names": [0],
    "func-style": [1, "expression"],
    "id-blacklist": [0],
    "id-length": [1, { "min": 2, "properties": "never", "exceptions": ["_", "i", "v", "k", "x", "y"] }],
    "id-match": [0],
    "indent": [1, 2, { "SwitchCase": 1 }],
    "jsx-quotes": [0],
    "key-spacing": [1, { "beforeColon": false, "afterColon": true }],
    "keyword-spacing": [1],
    "linebreak-style": [1, "unix"],
    "lines-around-comment": [0],
    "max-depth": [0, 4],
    "max-len": [0, 200],
    "max-nested-callbacks": [1],
    "max-params": [0, 10],
    "max-statements-per-line": [1, { "max": 1 }],
    "max-statements": [0],
    "multiline-ternary": [0],
    "new-cap": [1, { "newIsCap": true, "capIsNew": false }],
    "new-parens": [1],
    "newline-after-var": [1, "always"],
    "newline-before-return": [0],
    "newline-per-chained-call": [0],
    "no-array-constructor": [1],
    "no-bitwise": [1],
    "no-continue": [1],
    "no-inline-comments": [0],
    "no-lonely-if": [0],
    "no-mixed-spaces-and-tabs": [1],
    "no-multiple-empty-lines": [0],
    "no-negated-condition": [0],
    "no-nested-ternary": [0],
    "no-new-object": [1],
    "no-plusplus": [0],
    "no-restricted-syntax": [1],
    "no-tabs": [1],
    "no-ternary": [0],
    "no-trailing-spaces": [1],
    "no-underscore-dangle": [0, { "allowAfterThis": true, "allowAfterSuper": true, "allow": ["_"] }],
    "no-unneeded-ternary": [1],
    "no-whitespace-before-property": [1],
    "object-curly-newline": [0],
    "object-curly-spacing": [1, "always"],
    "one-var-declaration-per-line": [1],
    "one-var": [ 1, "never" ],
    "operator-assignment": [1, "always"],
    "operator-linebreak": [0],
    "padded-blocks": [0],
    "quote-props": [0],
    "quotes": [1, "single"],
    "require-jsdoc": [0],
    "semi-spacing": [1, { "before": false, "after": true }],
    "semi": [1, "always"],
    "sort-keys": [0],
    "sort-vars": [0],
    "sort-imports": [0],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "always"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": [1, {"int32Hint": false}],
    "space-unary-ops": [1, { "words": true, "nonwords": false }],
    "spaced-comment": [0],
    "unicode-bom": [1, "never"],
    "wrap-regex": [0],

    "arrow-body-style": [0, "as-needed"],
    "arrow-parens": [1, "always"],
    "arrow-spacing": [1],
    "constructor-super": [1],
    "generator-star-spacing": [1],
    "no-class-assign": [1],
    "no-confusing-arrow": [0],
    "no-const-assign": [1],
    "no-dupe-class-members": [1],
    "no-duplicate-imports": [1],
    "no-new-symbol": [1],
    "no-restricted-imports": [1],
    "no-this-before-super": [1],
    "no-useless-computed-key": [1],
    "no-useless-constructor": [0],
    "no-useless-rename": [0],
    "no-var": [1],
    "object-shorthand": [1, "always"],
    "prefer-arrow-callback": [1],
    "prefer-const": [1],
    "prefer-reflect": [0],
    "prefer-rest-params": [1],
    "prefer-spread": [1],
    "prefer-template": [1],
    "require-yield": [0],
    "template-curly-spacing": [1, "never"],
    "yield-star-spacing": [1, "both"]
  }
}
